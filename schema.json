{
  "elements"		: [
      {
    	"name": "Wrapper",
    	"handle": "wrapper",
    	"customizations": "all",
    	"hoverCustomizations": true,
      },
      {
    	"name": "FAQ Entry",
    	"handle": "entry",
    	"customizations": "all",
    	"hoverCustomizations": true,
    	"activeCustomizations": true
      },
      {
    	"name": "FAQ (open)",
    	"handle": "entry-open",
    	"customizations": "all",
    	"hoverCustomizations": true,
    	"activeCustomizations": true
      },
      {
    	"name": "Question",
    	"handle": "question",
    	"customizations": "all",
    	"hoverCustomizations": true,
    	"activeCustomizations": true,
        "defaults": {
          "font-size":1.3,
          "font-weight":500,
          "border-width": [0, 0, 1, 0],
          "padding":8,
          "border-color":"=text"
        },
        "defaultsHover": {
          "color": "=primary",
        }
      },
      {
    	"name": "Question (open)",
    	"handle": "question-open",
    	"customizations": "all",
    	"hoverCustomizations": true,
    	"activeCustomizations": true,
        "defaults": {
          "color": "=primary",
          "border-color": "=primary"
        }
      },
      {
    	"name": "Icon",
    	"handle": "icon",
    	"customizations": "box",
    	"hoverCustomizations": true,
    	"activeCustomizations": true
      },
      {
    	"name": "Answer",
    	"handle": "answer",
    	"customizations": "all",
    	"hoverCustomizations": true,
        "defaults": {
          "border-width": [0, 0, 1, 0],
          "padding":8,
          "border-color":"=text"
        },
      }
	],
	"properties"	: [
      {
          "name"		: "Only allow one question to be open at a time",
          "variable"	: "onlyOne",
          "type"		: "checkbox",
          "default"		: true
      },
      {
          "name"		: "Icon style",
          "variable"	: "iconStyle",
          "type"		: "dropdown",
          "default"		: "plus-minus",
          "options": [
            {
              "name": "Plus / Cross",
              "value": "plus-cross",
            },
            {
              "name": "Plus / Minus",
              "value": "plus-minus",
            },
            {
              "name": "Caret",
              "value": "caret",
            },
          ]
      },
      {
		  "name": "Question 1",
		  "type": "section",
		  "collapsed": false,
		  "fields"		: [
    		{
    			"name"			: "Question",
    			"variable"		: "question1",
    			"type"			: "text",
    			"default"		: "What is your return policy?"
    		},
    		{
    			"name"			: "Answer",
    			"variable"		: "answer1",
    			"type"			: "richText",
    			"default"		: "We offer a 30-day return policy on most of our products. Items must be returned in their original condition and packaging. To initiate a return, please visit our Returns & Exchanges page and follow the instructions provided. For more information, check out our Return Policy page."
    		},
            {
                "name"		: "Open by default",
                "variable"	: "openFirst",
                "type"		: "checkbox",
                "default"	: true
            },
          ]
      },
